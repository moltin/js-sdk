{
  "name": "@moltin/sdk",
  "description": "SDK for the Moltin eCommerce API",
  "version": "0.0.0-semantic-release",
  "homepage": "https://github.com/moltin/js-sdk",
  "author": "Moltin (https://moltin.com/)",
  "scripts": {
    "commit": "git-cz",
    "precommit": "lint-staged",
    "rollup": "rollup -c",
    "start": "rollup -c -w",
    "test": "mocha --compilers js:babel-core/register test/tests.js",
    "test-output":
      "mocha --compilers js:babel-core/register --reporter mocha-junit-reporter test/tests.js"
  },
  "main": "dist/moltin.cjs.js",
  "module": "src/moltin.js",
  "cjs:main": "dist/moltin.cjs.js",
  "browser": "dist/moltin.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/moltin/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/moltin/js-sdk/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.17.0",
    "nock": "^9.0.2",
    "prettier": "^1.11.1",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "cuid": "^2.1.0",
    "es6-promise": "^4.0.5",
    "fetch-everywhere": "^1.0.5",
    "inflected": "^2.0.1",
    "node-localstorage": "^1.3.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "directories": {
    "test": "test"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,json,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "jsxBracketSameLine": true
  }
}
