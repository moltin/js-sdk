{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "class-methods-use-this": 0,
    "import/no-named-as-default": 0,
    "camelcase": 0,
    "prefer-rest-params": 0,
    "import/no-extraneous-dependencies": 0,
    "no-unused-expressions": 0,
    "no-undef": 0,
    "global-require": 0,
    "no-bitwise": 0
  }
}
